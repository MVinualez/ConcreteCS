<Window x:Name="ConcreteCS" x:Class="ConcreteCS.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ConcreteCS"
        mc:Ignorable="d"
        Title="ConcreteCS" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Image HorizontalAlignment="Right" Height="55" Margin="0,0,674,369" VerticalAlignment="Bottom" Width="116" Source="/logo.png" Grid.ColumnSpan="2"/>
        <TabControl Grid.Column="1" HorizontalAlignment="Center" Height="369" Margin="0,65,0,0" VerticalAlignment="Top" Width="800">
            <TabItem Header="Calculs">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="Simulation">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
